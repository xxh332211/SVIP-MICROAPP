/* pages-userInfo/components/totalOrder/totalOrder.wxss */

.head {
  width: 100%;
  height: 94rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.head .title {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-weight: bold;
}

.head .title .ico {
  width: 42rpx;
  height: 42rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-right: 20rpx;
}

.head .zh {
  width: 42rpx;
  height: 42rpx;
  background-image: url(https://img.51jiabo.com/2c8128a4-762e-495c-948c-f4be3587cf8c.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-right: 20rpx;
}

.head .yy {
  background-image: url(https://img.51jiabo.com/da79b1e1-bc3b-413f-bf39-b417d13488e5.png);
}

.head .dj {
  background-image: url(https://img.51jiabo.com/11e0b497-82d7-4b5f-a21b-bb8f977fa04d.png);
}

.head .qk {
  background-image: url(https://img.51jiabo.com/ccea010f-bac8-4abe-aef3-f2f21f5cab42.png);
}

.head .xs {
  background-image: url(https://img.51jiabo.com/8de1f567-da63-4bfe-956e-b895373a125a.png);
}

.head .hg {
  background-image: url(https://img.51jiabo.com/a08b3b30-6608-42ee-b087-2ab323320efc.png);
}

.head .dh {
  background-image: url(https://img.51jiabo.com/8cc6365a-a620-4eba-9c31-f16641a9b643.png);
}

.status-text {
  font-size: 28rpx;
  font-weight: bold;
  color: #E5002D;
}

.content {
  display: flex;
  padding-bottom: 30rpx;
}

.goods-img {
  flex-shrink: 0;
  width: 200rpx;
  height: 200rpx;
  margin-right: 18rpx;
}

.right {
  width: 100%;
  position: relative;
}

.has-lottery {
  width: 126rpx;
  height: 126rpx;
  background: url(https://img.51jiabo.com/04152462-b133-423f-9a80-2d236ecd12ea.png) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  bottom: 0;
  right: 0;
}

.name {
  height: 76rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-size: 28rpx;
  line-height: 39rpx;
}

.price-box {
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 9rpx;
}

.more-choose-one-msg {
  font-size: 24rpx;
  color: #E6002D;
}

.price-box .origin {
  font-size: 24rpx;
  color: #9A9A9A;
  text-decoration: line-through;
  font-weight: normal;
  margin-left: 11rpx;
}

.btn-box {
  display: flex;
  justify-content: flex-end;
  margin-top: 13rpx;
}

.btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 120rpx;
  height: 56rpx;
  border: 1px solid #959595;
  border-radius: 28rpx;
  font-size: 24rpx;
  margin-left: 20rpx;
}

.btn.to-pay {
  width: 160rpx;
  background: #E6002D;
  border: none;
  color: #FFFFFF;
  box-shadow: 0 3rpx 9rpx 0 rgba(230, 0, 45, 0.35);
}

.btn.disable {
  background: #EEEEEE;
  color: #6D6D6D;
}

.btn.wide {
  width: 150rpx;
}

.draw-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx;
  height: 56rpx;
  background: #E6002D;
  box-shadow: 0 3rpx 9rpx 0 rgba(230, 0, 45, 0.35);
  border-radius: 28rpx;
  font-size: 28rpx;
  color: #FFFFFF;
}

.redeem-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx;
  height: 56rpx;
  border: 1px solid #E6002D;
  border-radius: 28rpx;
  font-size: 28rpx;
  color: #E6002D;
}

.order-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  font-size: 24rpx;
  border-top: 1px solid #EEEEEE;
  padding: 14rpx 0 24rpx;
}

/* 退款提示弹层 */
.refund-popup {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 30;
  background: rgba(0, 0, 0, 0.7);
}

.refund-popup>.refund-popup-box {
  width: 680rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 60rpx 50rpx 50rpx;
}

.refund-popup>.refund-popup-box>.title {
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  margin-bottom: 50rpx;
}

.refund-popup>.refund-popup-box>.content {
  font-size: 32rpx;
  width: 100%;
  height: 338rpx;
  margin-bottom: 82rpx;
}

.refund-popup>.refund-popup-box>.refund-popup-btns {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.refund-popup>.refund-popup-box .rpb-btn {
  width: 280rpx;
  height: 86rpx;
  line-height: 86rpx;
  font-size: 32rpx;
  text-align: center;
  border-radius: 3rpx;
}

.refund-popup>.refund-popup-box .rpb-cancel {
  border: 1rpx solid #C9C9C9;
  color: #222;
}

.refund-popup>.refund-popup-box .rpb-confirm {
  background-color: #E5002D;
  color: #fff;
}

.refund-popup>.refund-popup-box .rpb-dis-confirm {
  background-color: #D0D0D0;
  color: #FFFFFF;
}

.refund-popup>.refund-popup-box>.rpb-know {
  margin: 0 auto;
  background-color: #E5002D;
  color: #fff;
}

/* 退款提示 */

.refound-tips {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 29;
  background: rgba(0, 0, 0, 0.7);
}

.refound-tips .tips-box {
  width: 620rpx;
  background: #fff;
  border-radius: 8rpx;
  padding-top: 53rpx;
  padding-bottom: 53rpx;
}

.refound-tips .tips-box .tit {
  color: #000;
  font-weight: bold;
  font-size: 36rpx;
  text-align: center;
}

.refound-tips .tips-box .btn-box {
  display: flex;
  justify-content: center;
  margin-top: 77rpx;
}

.refound-tips .tips-box .btn-box .popup-btn {
  width: 190rpx;
  height: 70rpx;
  border-radius: 3rpx;
  font-size: 36rpx;
  background: #e6002d;
  color: #fff;
  text-align: center;
  line-height: 70rpx;
}

.refound-tips .tips-box .btn-box .cancel {
  border: 1px solid #aaa;
  background: #fff;
  color: #000;
  margin-right: 94rpx;
}

/* N选一弹层 */
.more-choose-one-popup {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 29;
  background: rgba(0, 0, 0, 0.7);
}

.more-choose-one-popup>.main {
  position: relative;
  width: 680rpx;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.more-choose-one-popup>.main>.title {
  width: 100%;
  height: 126rpx;
  line-height: 126rpx;
  text-align: center;
  font-size: 42rpx;
  font-weight: bold;
  color: #222;
  background-color: #F8F8F8;
  margin-bottom: 60rpx;
}

.more-choose-one-popup>.main .sc-view {
  width: 100%;
  white-space: nowrap;
}

.more-choose-one-popup>.main .item {
  width: 260rpx;
  margin-left: 20rpx;
  display: inline-block;
}

.more-choose-one-popup>.main .item:last-of-type {
  margin-right: 20rpx;
}

.more-choose-one-popup>.main .pro-img {
  display: block;
  width: 100%;
  height: 205rpx;
  margin-bottom: 16rpx;
}

.more-choose-one-popup>.main .desc {
  font-size: 24rpx;
  color: #222;
  height: 65rpx;
  line-height: 34rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  white-space: normal;
}

.more-choose-one-popup>.main .price {
  display: flex;
  align-items: baseline;
  color: #E6002D;
  font-size: 28rpx;
  font-weight: bold;
  margin: 13rpx 0 28rpx;
  padding-top: 20rpx;
  border-top: 1rpx solid #EFEFEF;
}

.more-choose-one-popup>.main .num {
  font-size: 36rpx;
}

.more-choose-one-popup>.main .unit {
  font-size: 20rpx;
  font-weight: normal;
  letter-spacing: 2rpx;
  padding-left: 4rpx;
}

.more-choose-one-popup>.main .buy-btn {
  width: 100%;
  height: 65rpx;
  line-height: 65rpx;
  border-radius: 2rpx;
  font-size: 28rpx;
  color: #fff;
  background-color: #E6002D;
  margin-top: 28rpx;
}

.more-choose-one-popup>.main .dis-buy-btn {
  background-color: #D0D0D0;
}

.more-choose-one-popup>.main .mco-btn {
  width: 280rpx;
  height: 86rpx;
  line-height: 86rpx;
  border-radius: 4rpx;
  background-color: #E5002D;
  color: #fff;
  font-size: 32rpx;
  margin: 40rpx auto 60rpx;
}

.more-choose-one-popup>.main>.mco-close {
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 30rpx;
  height: 30rpx;
}

.more-choose-one-tips {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 30;
  background: rgba(0, 0, 0, 0.7);
}

.more-choose-one-tips>.mco-box{
  position: relative;
  width: 680rpx;
  background-color: #fff;
  border-radius: 8rpx;
  text-align: center;
  padding-bottom: 50rpx;
}

.more-choose-one-tips .mco-tips-btn{
  width: 280rpx;
  height: 86rpx;
  line-height: 86rpx;
  color: #fff;
  font-size: 32rpx;
  background-color: #E5002D;
  margin: 50rpx auto 0;
}

.more-choose-one-tips>.mco-box1>.mco-tips-title{
  font-size: 36rpx;
  margin-top: 90rpx;
}

.more-choose-one-tips>.mco-box2>.mco-tips-text{
  font-size: 32rpx;
}

.more-choose-one-tips>.mco-box2>.mco-tips-text:first-of-type{
  margin: 70rpx 0 20rpx;
}

.more-choose-one-tips>.mco-box2>.mco-tips-text:last-of-type{
  margin-top: 40rpx;
}

.more-choose-one-tips .mco-close{
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 30rpx;
  height: 30rpx;
}

/* 提示弹窗 */
.tips-box-pop {
  max-width: 450rpx;
  color: #fff;
  text-align: center;
  background: rgba(51, 51, 51, .9);
  border-radius: 10rpx;
  z-index: 999;
  padding: 20rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 26rpx;
  box-sizing: border-box;
  white-space: normal;
}