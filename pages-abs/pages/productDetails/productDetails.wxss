/* pages-abs/pages/productDetails/productDetails.wxss */
@import '../../subcontracting.wxss';
.navigate {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 84rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    box-sizing: content-box;
  }
  .navigate.showBack {
    background-color: #fff;
  }
  .navigate .return {
    width: 75rpx;
    height: 60rpx;
    background: url("https://img.51jiabo.com/10c74c3b-3825-4d97-a58b-6ee8cbcdf434.png") no-repeat;
    background-size: 20rpx 35rpx;
    background-position: 20rpx center;
    position: absolute;
    bottom: 14rpx;
    left: 4rpx;
    padding-left: 20rpx;
    padding-right: 35rpx;
  }
  
  .navigate .nav-text {
    font-size: 30rpx;
    font-weight: bold;
    max-width: 380rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
  }
  
  .navigate-blank {
    width: 100%;
    height: 82rpx;
    box-sizing: content-box;
  }
.banner {
    width: 750rpx;
    height: 540rpx;
    position: relative;
}

.banner video,
.banner swiper,
.banner swiper-item {
    width: 100%;
    height: 100%;
}

.indicate {
    position: absolute;
    bottom: 20rpx;
    right: 20rpx;
    background: rgba(0, 0, 0, .5);
    z-index: 1;
    border-radius: 30rpx;
    color: #fff;
    padding: 5rpx 20rpx;
}

.price {
    padding: 0 30rpx;
