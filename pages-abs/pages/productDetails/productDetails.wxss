/* 到店自提更多门店弹层 */
.store-popup {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, .4);
    z-index: 103;
    transition: all .2s linear;
    visibility: hidden;
}

.store-popup.show {
    visibility: visible;
}

.store-popup.show .store-box {
    height: 552rpx;
}

.store-popup .store-box {
    width: 100%;
    height: 0;
    background: #FFFFFF;
    border-radius: 27rpx 27rpx 0 0;
    transition: all .2s linear;
    position: absolute;
    bottom: 0;
    overflow: hidden;
    padding: 28rpx;
}

.store-popup .store-close {
    width: 107rpx;
    height: 107rpx;
    background: url(https://img.51jiabo.com/e54c2376-7d89-4477-adbe-fecaf0007617.png) no-repeat;
    background-size: 27rpx 27rpx;
    background-position: center center;
    position: absolute;
    top: 0;
    right: 0;
}

.store-popup .store-title {
    font-size: 32rpx;
    font-weight: bold;
}

.store-popup .store-title text {
    font-size: 30rpx;
    font-weight: normal;
    margin-left: 14rpx;
}

.store-popup .store-list {
    height: 480rpx;
    overflow-y: scroll;
    padding-bottom: 30rpx;
}

.store-popup .store-item {
    font-size: 26rpx;
    margin-top: 24rpx;
}

/* 领取门票弹层 */
.ticket-popup {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, .4);
    z-index: 103;
}

.ticket-popup .ticket-box {
    width: 570rpx;
    height: 556rpx;
    position: relative;
}

.ticket-popup .bg {
    width: 100%;
}

.ticket-popup .head {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
    height: 203rpx;
    background: #F81C50;
    border-radius: 30rpx 30rpx 0 0;
    position: absolute;
    top: 0;
    font-size: 30rpx;
    color: #FFFEFF;
    padding-top: 49rpx;
}

.ticket-popup .bold {
    font-weight: bold;
}

.ticket-popup .ticket {
    width: 440rpx;
    height: 202rpx;
    position: absolute;
    top: 176rpx;
    left: 70rpx;
}

.ticket-popup .get-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 288rpx;
    height: 90rpx;
    background: #F81C50;
    border-radius: 10rpx;
    font-size: 30rpx;
    color: #FFFEFF;
    position: absolute;
    bottom: 43rpx;
    left: 140rpx;
}

.ticket-popup .ticket-close {
    width: 65rpx;
    height: 65rpx;
    position: absolute;
    bottom: -90rpx;
    left: 252rpx;
    background: url(https://img.51jiabo.com/f292d96f-67b2-4eed-adb8-1fd4e43a6b81.png) no-repeat;
    background-size: 100% 100%;
}