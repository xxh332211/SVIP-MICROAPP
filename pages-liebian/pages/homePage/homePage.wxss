  height: 21rpx;
  background: #E51613;
  font-size: 22rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 21rpx;
}

.ticket-box .ticket-list .item .win-text {
  color: #E6002D;
  font-size: 36rpx;
  font-weight: bold;
  line-height: 1;
  padding-bottom: 8rpx;
}

.ticket-box .click-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 623rpx;
  height: 107rpx;
  background: #E6002D;
  border-radius: 50rpx;
  font-size: 40rpx;
  font-weight: bold;
  color: #FFFFFF;
  margin-top: 32rpx;
  margin-left: 33rpx;
}

.ticket-box .click-btn.gray {
