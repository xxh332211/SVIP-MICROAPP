button {
  padding: 0;
  background-color: transparent;
  border: none;
}

.navigate {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 84rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  box-sizing: content-box;
}

.navigate .return {
  width: 75rpx;
  height: 60rpx;
  background: url("https://img.51jiabo.com/3841b3ab-d83e-492f-adca-7bed46b86ead.png") no-repeat;
  background-size: 20rpx 35rpx;
  background-position: 20rpx center;
  position: absolute;
  bottom: 14rpx;
  left: 4rpx;
  z-index: 2;
  padding-left: 20rpx;
  padding-right: 35rpx;
}

.navigate .nav-text {
  color: #fff;
  font-size: 30rpx;
  font-weight: bold;
}

.fixed-btn {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  position: absolute;
  top: 90rpx;
  right: 0;
  z-index: 2;
}

.fixed-btn .log {
  width: 140rpx;
  height: 118rpx;
  background: url(https://img.51jiabo.com/3acd0b0c-0db7-45cf-9383-1770cb35e7f5.png) no-repeat;
  background-size: 100% 100%;
}

.fixed-btn .rule {
  width: 97rpx;
  height: 118rpx;
  background: url(https://img.51jiabo.com/aac608fb-9a11-488d-bf9e-87fd3b68b831.png) no-repeat;
  background-size: 100% 100%;
  margin-top: -29rpx;
}

.wrapper {
  /* background-color: #F03152; */
}

.ticket-box {
  width: 689rpx;
  background: #FFFFFF;
  box-shadow: 0 0 18rpx 0 rgba(56, 39, 43, 0.25);
  border-radius: 30rpx;
  position: relative;
  z-index: 1;
  margin-left: 30rpx;
  margin-top: -114rpx;
}

.ticket-box .title {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  font-weight: bold;
  font-style: italic;
  padding-top: 30rpx;
}

.ticket-box .title.fs {
  font-size: 48rpx;
}

.ticket-box .title .time {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 42rpx;
  font-weight: normal;
  /* font-style: normal; */
  color: #FFFFFF;
  width: 68rpx;
  height: 50rpx;
  background: url(https://img.51jiabo.com/0b8b52da-2967-43bc-b4ef-5adb3a7b577a.png) no-repeat;
  background-size: 100% 100%;
  letter-spacing: 10rpx;
  padding-left: 4rpx;
  margin: 0 6rpx 0 10rpx;
}

.ticket-box .title .time:nth-child(1) {
  margin-left: 12rpx;
}

.ticket-box .tips {
  font-size: 28rpx;
  color: #E6002D;
  text-align: center;
}

.ticket-box .ticket-list {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.ticket-box .ticket-list .item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 198rpx;
  height: 99rpx;
  background-image: url(https://img.51jiabo.com/5abfca5b-a779-47d2-b7fb-68f1f69ce683.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  font-size: 30rpx;
  color: #8A8A8A;
  margin: 25rpx 5rpx 0 5rpx;
}

.ticket-box .ticket-list .item.has {
  color: #FFFFFF;
  background-image: url(https://img.51jiabo.com/9ae182d2-1913-4238-8e6a-3b6c416115aa.png);
}

.ticket-box .ticket-list .item.win {
  background-image: url(https://img.51jiabo.com/bfd358e9-2162-4894-be3e-3a7f01554b38.png);
}

.ticket-box .ticket-list .item.win::before {
  content: "";
  display: block;
  width: 35rpx;
  height: 31rpx;
  background: url(https://img.51jiabo.com/f04f1139-75b9-4578-b7ba-4c4a2be23a86.png) no-repeat;
  background-size: 100% 100%;
  margin-top: -17rpx;
}

.ticket-box .ticket-list .item .ticket-num {
