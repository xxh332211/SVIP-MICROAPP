  justify-content: center;
  width: 100%;
  height: 84rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 102;
  box-sizing: content-box;
}

.navigate .city {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 50rpx;
  background: rgba(0, 0, 0, 0.5);
