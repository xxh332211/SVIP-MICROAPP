/* pages/expoPackage//ticketCheck/ticketCheck.wxss */
.head {
  background-color: #fff;
  padding: 73rpx 0 15rpx;
}

.ticket-title {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 38rpx;
  color: #222222;
  font-weight: bold;
}

.two-line {
  margin-top: -28rpx;
  margin-bottom: -36rpx;
}

.two-line .svip-title {
  margin-bottom: 4rpx;
}

.ticket-title .ico {
  width: 54rpx;
  height: 54rpx;
  background-image: url(https://img.51jiabo.com/e4d8290b-f516-4fcb-adfa-845db461b902.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-right: 16rpx;
}

.ticket-title .ico.fail {
  background-image: url(https://img.51jiabo.com/3fbdb621-a793-425e-b49c-d32febe5aba6.png);
}

.btn-box {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-left: 30rpx;
  margin-top: 81rpx;
  margin-bottom: 42rpx;
}

.btn-box .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 310rpx;
  height: 83rpx;
  background-color: #E6002D;
  box-shadow: 0 20rpx 20rpx 0 rgba(204, 13, 50, 0.2);
  border-radius: 5rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  padding: 0;
  margin: 0 40rpx 0 10rpx;
}

.btn-box .btn.one {
  width: 400rpx;
}

.sign-gift {
  width: 100%;
  overflow: hidden;
  margin-top: 20rpx;
}

.sign-title {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #222222;
  background-color: #fff;
  padding-left: 30rpx;
}

.sign-title .tips {
  width: 65rpx;
  height: 31rpx;
  background: url(https://img.51jiabo.com/7cce72f0-81a0-4061-9173-a329f0ee79b0.png) no-repeat;
  background-size: 100% 100%;
  margin-right: 17rpx;
}

.sign-title .red{
  color: #E6002D;
}

.content {
  height: 386rpx;
  margin: 20rpx auto 0;
  text-align: center;
  background: url(https://img.51jiabo.com/4307d05a-9789-4c87-954b-a39a09bc8aff.png) no-repeat;
  background-size: cover;
  padding: 60rpx 0 30rpx;
}

.content .content-title {
  color: #222;
  font-weight: bold;
  font-size: 30rpx;
}

.content .red {
  color: #E6002D;
  font-weight: bold;
  font-size: 74rpx;
}

.content .small {
  font-size: 26rpx;
  margin-top: 38rpx;
  color: #222;
}

/* 签到礼未抽奖 */

.not-sign image {
  display: block;
  width: 700rpx;
  margin-left: -5rpx;
  margin-top: 30rpx;
}

.adv {
  width: 100%;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  padding: 30rpx 30rpx 0;
}

.adv.three .item {
  width: 220rpx;
}

.adv.two .item {
  width: 340rpx;
}

.adv.one .item {
  width: 100%;
}

.adv .item {
  background-color: #E4E4E4;
  height: 150rpx;
}

.acrco-box {
  background-color: #fff;
}

.share-popup {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  z-index: 1;
  background-color: rgba(0, 0, 0, .8);
}

.share-popup .share-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 640rpx;
  background-color: #fff;
  border-radius: 10rpx;
  position: relative;
  padding: 85rpx 0 38rpx;
}

.share-popup .close {
  width: 34rpx;
  height: 34rpx;
  background: url(https://img.51jiabo.com/345d4b82-b7a8-477c-83f5-1568ceaa9da1.png) no-repeat;
  background-size: 100% 100%;
  position: absolute;
  top: 35rpx;
  right: 31rpx;
}

.share-popup .mini-code {
  width: 200px;
  height: 200px;
}

.share-popup .text {
  font-size: 42rpx;
  color: #222222;
  font-weight: bold;
  margin-top: 40rpx;
}

.tips-popup {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  z-index: 1;
  background-color: rgba(0, 0, 0, .8);
}

.tips-popup .tips-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 620rpx;
  border-radius: 8rpx;
  background-color: #ffff;
  padding: 90rpx 0 87rpx;
}

.tips-popup .status {
  display: flex;
  align-items: center;
  font-size: 42rpx;
  font-weight: bold;
  color: #E6002D;
}

.tips-popup .ico {
  width: 54rpx;
  height: 54rpx;
  background-image: url(https://img.51jiabo.com/e4d8290b-f516-4fcb-adfa-845db461b902.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-right: 28rpx;
}

.tips-popup .detail {
  font-size: 42rpx;
  font-weight: bold;
  color: #222222;
  margin-top: 10rpx;
}

.acrco-img {
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 20rpx;
}

.svip-popup {
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .8);
}

.svip-popup .svip-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 640rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  position: relative;
  padding-top: 74rpx;
  padding-bottom: 45rpx;
}

.svip-popup .close {
  width: 81rpx;
  height: 81rpx;
  background: url(https://img.51jiabo.com/0118c3a3-525f-4e5e-9507-b332c2f5340f.png) no-repeat;
  background-size: 27rpx 27rpx;
  background-position: center;
  position: absolute;
  top: 0;
  right: 0;
}

.svip-popup .svip-title {
  font-size: 32rpx;
  color: #22221F;
}

.svip-popup .to-center {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 280rpx;
  height: 86rpx;
  background: #E6002D;
  border-radius: 3rpx;
  color: #FFFFFF;
  font-size: 32rpx;
  margin-top: 43rpx;
}

/* 抽奖弹层 */
.sign-popup {
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 30;
  background: rgba(0, 0, 0, .8);
  width: 100%;
  height: 100%;
}

.sign-popup .sign-box {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.sign-popup image {
  width: 700rpx;
  margin-bottom: 40rpx;
}

.sign-popup .close {
  width: 35rpx;
  height: 35rpx;
  background: url(https://img.51jiabo.com/6edfde28-dab7-4cfd-a0b2-a1d39f5063a8.png) no-repeat;
  background-size: 100% 100%;
}

.xmb-tips{
  width: 636rpx;
  background-color: #fff;
  border-radius: 10rpx;
  position: relative;
  margin: 30rpx auto;
  padding-bottom: 20rpx;
  overflow: hidden;
}

.xmb-tips-banner{
  width: 100%;
  height: 290rpx;
  color: #fff;
  text-align: center;
  background: url(https://img.51jiabo.com/4c83a049-899e-4b83-98f2-9fb782fe5f2c.png) no-repeat;
  background-size: cover;
}

.xmb-tips-off-banner{
  height: 248rpx;
  background: url(https://img.51jiabo.com/e68c81f0-5b2b-4c2d-b4e8-f67ca457ce96.png) no-repeat;
  background-size: cover;
  line-height: 125rpx;
  margin-bottom: 50rpx;
}

.xmb-tips-title{
  padding: 60rpx 0 50rpx;
  font-size: 48rpx;
  font-weight: bold;
}

.xmb-tips-desc{
  font-size: 32rpx;
}

.xmb-tips-img{
  display: block;
  width: 390rpx;
  height: 290rpx;
  transform: scale(.8);
  margin: -70rpx auto 0;
}

.xmb-tips-btn{
  width: 270rpx;
  height: 75rpx;
  background-color: #FF0036;
  text-align: center;
  line-height: 75rpx;
  border-radius: 75rpx;
  color: #fff;
  font-size: 32rpx;
}

.xmb-tips-close{
  position: absolute;
  top: 22rpx;
  right: 27rpx;
  width: 30rpx;
  height: 30rpx;
}

.share-box {
  display: flex;
  flex-direction:column;
  justify-content: space-around;
}

.share-box .inviteBtn{
  width: 670rpx;
  height: 90rpx;
  margin-bottom: 40rpx;
  background: #E6002D;
  box-shadow:  0px 20rpx 20rpx #cc0d3226;
  border-radius: 5rpx;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  line-height: 32rpx;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: none;
  outline: none;
  -webkit-border-radius: 5rpx;
  -moz-border-radius: 5rpx;
  -ms-border-radius: 5rpx;
  -o-border-radius: 5rpx;
}

