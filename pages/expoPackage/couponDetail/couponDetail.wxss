/* pages/expoPackage/couponDetail/couponDetail.wxss */
.main-wrap {
  padding: 30rpx 30rpx 60rpx;
}

.coupon-wrap {
  width: 100%;
  height: 600rpx;
  background: url(https://img.51jiabo.com/9e9b7a53-a573-41e4-a193-21376f95da66.png) no-repeat;
  background-size: cover;
  background-position: 0 -64rpx;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  overflow: hidden;
}

.header {
  position: relative;
  display: flex;
  align-items: center;
  padding: 27rpx 20rpx;
  font-size: 32rpx;
  color: #fff;
  background: linear-gradient(to right, #FF0036, #FF2600);
}

.logo {
  width: 110rpx;
  height: 51rpx;
  border-radius: 6rpx;
  margin-right: 20rpx;
}

.logo-c {
  height: 40rpx;
}

.svip-icon {
  position: absolute;
  top: 0;
  right: -2rpx;
  width: 134rpx;
  height: 78rpx;
}

.content {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-top: 20rpx;
}

.price {
  font-size: 46rpx;
  font-weight: bold;
  color: #FF0137;
}

.desc {
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 50rpx;
  padding: 0 30rpx;
  background-color: #FFF4F6;
  color: #FF0137;
  font-size: 30rpx;
}

.expire-time {
  margin: 30rpx 0 100rpx;
  font-size: 26rpx;
  color: #222;
}

.tips {
  font-size: 26rpx;
  color: #222;
  padding: 0 20rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.rules-wrap {
  background-color: #fff;
  border-radius: 10rpx;
  padding: 40rpx 20rpx 100rpx;
  font-size: 28rpx;
  color: #222;
  line-height: 50rpx;
}

.rules-tit {
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.footer {
  padding: 34rpx 30rpx;
  background-color: #fff;
}

.btn {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 100rpx;
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
  color: #fff;
  background-color: #E6002D;
}

.dis-btn {
  background-color: #BFBFBF;
}

/* 优惠券领取成功弹层 */

.get-ticket-box {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.7);
}

.get-ticket-box .get-ticket {
  width: 620rpx;
  background: #fff;
  border-radius: 5rpx;
  position: relative;
  padding: 37rpx 0 56rpx;
}

.get-ticket-box .get-ticket .txt-box {
  text-align: center;
  border-bottom: 1px solid #dedede;
  padding-bottom: 33rpx;
}

.get-ticket-box .get-ticket .txt-box .tit1 {
  font-size: 42rpx;
  font-weight: bold;
}

.get-ticket-box .get-ticket .txt-box .tit2 {
  font-size: 28rpx;
  margin-top: 7rpx;
  padding: 0 60rpx;
}

.coupon-box {
  display: flex;
  width: 529rpx;
  height: 153rpx;
  box-shadow: 3px 0 15rpx 5rpx rgba(81, 81, 81, 0.05);
  margin-top: 43rpx;
  margin-left: 45rpx;
}

.coupon-box .left {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 192rpx;
  height: 153rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  color: #fff;
}

.coupon-box .left .price {
  display: flex;
  align-items: baseline;
  font-size: 52rpx;
  font-weight: bold;
  margin-top: 4rpx;
}

.coupon-box .left .price .lab {
  font-size: 31rpx;
  font-weight: normal;
}

.coupon-box .left .cond {
  font-size: 17rpx;
  margin-top: -10rpx;
}

.coupon-box .right {
  display: flex;
  flex-direction: column;
  margin-left: 65rpx;
}

.coupon-box .right .s-img {
  width: 94rpx;
  height: 51rpx;
  border: 1px solid #e5e5e5;
  margin-top: 15rpx;
}

.coupon-box .right .name {
  font-size: 23rpx;
  margin-top: 8rpx;
}

.coupon-box .right .date {
  font-size: 17rpx;
  color: #666;
  margin-top: 2rpx;
}

.get-ticket .share {
  width: 530rpx;
  height: 83rpx;
  border-radius: 5rpx;
  background: #e6002d;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 83rpx;
  margin-top: 28rpx;
  margin-left: 45rpx;
}

.get-ticket .remaind {
  font-size: 24rpx;
  color: #777;
  margin-top: 44rpx;
  padding: 0 45rpx;
}

.get-ticket .remaind .remaind-txt {
  color: #e6002d;
  font-weight: bold;
}

.get-ticket .close {
  position: absolute;
  top: -50rpx;
  right: -50rpx;
  width: 99rpx;
  height: 99rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}