/* pages/user/selfHelp/FillIn/FillIn.wxss */
.box{
  padding-bottom: 200rpx;
}
.top {
  width: 100%;
  height: 220rpx;
  background: #ff0036;
}

.top .title {
  display: flex;
  align-items: center;
  font-size: 36rpx;
  color: #fff;
  font-weight: bold;
  padding: 34rpx 0 0 34rpx;
}


.top .title::before {
  content: "";
  display: block;
  width: 52rpx;
  height: 32rpx;
  background: url("https://img.51jiabo.com/170ec7ec-1936-4ab8-9361-4ed4a5e40fde.png") no-repeat;
  background-size: 32rpx 32rpx;
  margin-right: 10rpx;
}

.tab {
  display: flex;
  width: 690rpx;
  height: 90rpx;
  border-radius: 10rpx 10rpx 0 0;
  background: #d9002d;
}

.tab view {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50%;
  font-size: 30rpx;
  color: #fff;
  font-weight: bold;
  position: relative;
}

 .tab view.active {
  height: 98rpx;
  background: #fff;
  font-size: 36rpx;
  color: #222;
  border-radius: 10rpx 0 0 0;
  margin-top: -8rpx;
}

.tab view.active::before {
  content: "";
  display: block;
  width: 13rpx;
  height: 13rpx;
  background: #ff0036;
  border-radius: 50%;
  margin-right: 21rpx;
}

.tab view.active::after {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 0 45rpx 98rpx 0;
  border-color: transparent transparent #fff transparent;
  position: absolute;
  right: -43rpx;
  bottom: 0;
}

.tab .vendor.active {
  height: 98rpx;
  background: #fff;
  font-size: 36rpx;
  color: #222;
  border-radius: 0 10rpx 0 0;
  margin-top: -8rpx;
}
.tab .vendor.active::after {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 0 0 98rpx 45rpx;
  border-color: transparent transparent #fff transparent;
  position: absolute;
  left: -43rpx;
  bottom: 0;
} 

/* 表单 */

.form-box {
  background: #fff;
  width: 690rpx;
  border-radius: 10rpx;
  margin-top: -90rpx;
  margin-left: 30rpx;
  position: relative;
}

.form-box .fuzzySearch{
  position: absolute;
  top: 218rpx;
  left: 210rpx;
  width: 350rpx;
  background:#fff ;
  z-index: 99;
  font-size: 32rpx;
  border: 1rpx solid #cccc;
} 
.form-box .fuzzySearch .text{
  line-height: 80rpx;
  border-bottom: 1rpx solid #cccc;
}
.form-box .fuzzySearch .text:last-child{
  border-bottom: none;
}
.form-item {
  display: flex;
  align-items: center;
  margin: 0 20rpx;
  border-bottom: 1px solid #f0f0f0;
  position: relative;
  padding: 36rpx 0 36rpx 5rpx;
}

.form-item .name {
  width: 190rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.form-item .content {
  display: flex;
  flex-direction: column;
}

.form-item .content input {
  font-size: 32rpx;
  margin-top: 1px;
}

.form-item .chaInput{
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}

.form-item .content .error {
  font-size: 28rpx;
  color: #fc5c6b;
  margin-bottom: -16rpx;
}

.form-item .scan-img {
  width: 65rpx;
  height: 65rpx;
  position: absolute;
  top: 28rpx;
  right: 20rpx;
}

.form-item .q {
  width: 60rpx;
  height: 124rpx;
  position: absolute;
  top: 0;
  right: 0;
  background: url("https://img.51jiabo.com/08f42af1-6f66-4d0b-91ca-fdbaf99bde29.png") no-repeat;
  background-size: 32rpx 32rpx;
  background-position: center;
}

.amount {
  padding-bottom: 47rpx;
}

.amount .amount-top {
  display: flex;
  padding-left: 25rpx;
  padding-top: 34rpx;
}

.amount .amount-top .name {
  width: 190rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.amount .amount-top .content .way {
  display: flex;
}

.amount .amount-top .content .way .item {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #777;
}

.amount .amount-top .content .way .item:first-child {
  margin-right: 76rpx;
}

.amount .amount-top .content .label {
  width: 46rpx;
  height: 46rpx;
  background: url("https://img.51jiabo.com/450ef5ac-4001-4d0c-be90-2ad04ef83394.png") no-repeat;
  background-size: 100% 100%;
  margin-right: 22rpx;
}

.amount .amount-top .content .label.active {
  background: url("https://img.51jiabo.com/687c8a37-33af-4e6a-9e18-8d044f6f6ed9.png") no-repeat;
  background-size: 100% 100%;
}

.amount .amount-top .content .error {
  font-size: 28rpx;
  color: #fc5c6b;
  margin-top: 20rpx;
  margin-bottom: -14rpx;
}

.amount .amount-form {
  display: flex;
  align-items: center;
  width: 640rpx;
  height: 105rpx;
  background: #f9f9f9;
  position: relative;
  margin-left: 25rpx;
  margin-top: 32rpx;
}
.amount .amount-form .layer{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 3;
}
.amount .amount-form .symbols {
  font-size: 46rpx;
  font-weight: bold;
  position: absolute;
  top: 16rpx;
  left: 10rpx;
}

.amount .amount-form input {
  width: 70%;
  font-size: 48rpx;
  font-weight: bold;
  padding-left: 68rpx;
}

.amount .amount-form .placeholder {
  font-size: 30rpx;
  font-weight: normal;
}

.amount .amount-form .clear {
  width: 95rpx;
  height: 104rpx;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 2;
  background: url("https://img.51jiabo.com/1f3d2c94-dcc6-4ec1-84ec-73abbf1d4a7b.png") no-repeat;
  background-size: 30rpx 30rpx;
  background-position: center;
}

.goods-photo {
  width: 690rpx;
  margin-left: 30rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-top: 26rpx;
  padding-bottom: 50rpx;
}

.goods-name {
  padding: 29rpx 20rpx 0;
}

.goods-name .name {
  font-size: 32rpx;
  font-weight: bold;
  padding-left: 7rpx;
}

.goods-name .error {
  font-size: 28rpx;
  color: #fc5c6b;
  padding-left: 7rpx;
}

.goods-name textarea {
  width: 100%;
  height: 205rpx;
  box-sizing: border-box;
  border: 1px solid #dcdcdc;
  border-radius: 5rpx;
  margin-top: 19rpx;
  font-size: 30rpx;
  padding: 24rpx 30rpx;
}

.take-photo {
  padding: 41rpx 0 35rpx 20rpx;
}

.take-photo .name {
  font-size: 32rpx;
  font-weight: bold;
  padding-left: 7rpx;
}

.take-photo .error {
  font-size: 28rpx;
  color: #fc5c6b;
  padding-left: 7rpx;
}

.take-photo .photo {
  display: flex;
  margin-top: 24rpx;
}

.take-photo .photo .photo-item {
  width: 200rpx;
  height: 200rpx;
  position: relative;
  margin-right: 24rpx;
}

.take-photo .photo .photo-item image {
  width: 100%;
  height: 100%;
}

.take-photo .photo .photo-item .del {
  width: 40rpx;
  height: 40rpx;
  background: url("https://img.51jiabo.com/3e1b2729-8247-4503-b917-02f6bb65ffbd.png") no-repeat;
  background-size: 100%;
  position: absolute;
  top: 1px;
  right: 1px;
}

.take-photo .photo .take-btn {
  width: 200rpx;
  height: 200rpx;
  box-shadow: 0px 15px 20px 0px rgba(157, 157, 157, 0.2);
}

.take-photo .photo .take-btn .img {
  width: 98rpx;
  height: 93rpx;
  margin: 21rpx 0 0 47rpx;
}

.take-photo .photo .take-btn .order-img {
  font-size: 28rpx;
  color: #999;
  text-align: center;
  margin-top: 16rpx;
}

.rule{
  width: 690rpx;
  border-radius: 10rpx;
  background: #fff;
  margin: 23rpx 0 0 30rpx;
  padding: 38rpx 35rpx;
}
.rule .rule-tit{
  font-size: 32rpx;
  font-weight: bold;
  padding-bottom: 26rpx;
}
.rule .rule-content{
  font-size: 28rpx;
  line-height: 53rpx;
}

.submit-btn {
  width: 690rpx;
  height: 94rpx;
  background: #ff0036;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 94rpx;
  position: fixed;
  bottom: 50rpx;
  left: 30rpx;
  z-index: 2;
}

cover-view {
  display: block;
  line-height: 48rpx;
  white-space: normal;
}

.error-tips{
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  padding: 20rpx;
  background: rgba(0,0,0,.8);
  border-radius: 10rpx;
  font-size: 32rpx;
  color: #fff;
  z-index: 10;
  white-space: nowrap;
}

.phone-tips {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 10;
}

.phone-tips .phone-tips-box {
  width: 600rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 10rpx;
  padding: 70rpx 50rpx 72rpx;
}

.phone-tips .phone-tips-box .text {
  font-size: 32rpx;
  color: #777;
}

.close {
  width: 271rpx;
  height: 75rpx;
  border: 1px solid #ff0036;
  border-radius: 37rpx;
  font-size: 36rpx;
  color: #ff0036;
  font-weight: bold;
  text-align: center;
  line-height: 75rpx;
  margin-left: 115rpx;
  margin-top: 59rpx;
}

.tips-popup{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0,0,0,.7);
}
.tips-popup .tips-box{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 600rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 89rpx 0 37rpx;
}
.tips-popup .tit{
  width: 100%;
  text-align: center;
  font-size: 36rpx;
  color: #FF0036;
  font-weight: bold;
}
.tips-popup .content{
  font-size: 30rpx;
  color: #777777;
  padding: 22rpx 20rpx 67rpx;
  text-align: center;
  width: 100%;
  box-sizing: border-box;

}
.tips-popup .confirm{
  font-size: 36rpx;
  color: #777777;
  width: 271rpx;
  height: 75rpx;
  line-height: 75rpx;
  text-align: center;
  border: 1px solid #DCDCDC;
  border-radius: 37rpx;
}

.pass-tips{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.7);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.pass-tips .pass-box{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 600rpx;
  background: #fff;
  border-radius: 10rpx;
  padding-bottom: 35rpx;
}
.pass-tips .pass-box cover-image{
  width: 100%;
}
.pass-tips .pass-box .content{
  font-size: 32rpx;
  color: #777;
  text-align: center;
  margin-top: 53rpx;
}
.pass-tips .pass-box .confirm{
  width: 271rpx;
  height: 75rpx;
  border: 1px solid #DCDCDC;
  border-radius: 37rpx;
  font-size: 36rpx;
  color: #777;
  text-align: center;
  line-height: 75rpx;
  margin-top: 37rpx;
}