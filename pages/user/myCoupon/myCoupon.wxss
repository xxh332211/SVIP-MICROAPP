/* 券码弹层 */

.code-popup {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 29;
  background: rgba(0, 0, 0, 0.7);
}

.code-popup .code-box {
  width: 610rpx;
  background: url("https://img.51jiabo.com/cf8a82d1-8ad7-4232-b7e9-b51af6efa331.png") no-repeat;
  background-size: 100%;
  padding-bottom: 42rpx;
}

.code-popup .code-box .top {
  text-align: center;
}

.code-popup .code-box .top .tit {
  font-size: 36rpx;
  font-weight: bold;
  padding: 46rpx 0 0;
}

.code-popup .code-box .top .period {
  font-size: 30rpx;
  color: #e6002d;
  padding-bottom: 36rpx;
}

.code-popup .code-box .bottom {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.code-popup .code-box .bottom .canvas {
  width: 170px;
  height: 170px;
  margin-top: 55rpx;
  overflow: hidden;
  position: relative;
}

.code-popup .code-box .bottom .cou-code {
  font-size: 32rpx;
  margin-top: 20rpx;
}

.code-popup .code-box .bottom .cou-code.past {
  text-decoration: line-through;
}

.code-popup .code-box .bottom .status {
  width: 240rpx;
  height: 70rpx;
  border-radius: 5rpx;
  font-size: 36rpx;
  color: #fff;
  font-weight: bold;
  text-align: center;
  line-height: 70rpx;
  background: #e6002d;
  margin-top: 40rpx;
}

.code-popup .code-box .bottom .status.used {
  background: #f1c679;
}

.code-popup .code-box .bottom .status.past {
  background: #bababa;
}