/* pages/user/myCoupon/myCoupon.wxss */

page {
  background: #fff;
}

.tab-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-shadow: 0px 5rpx 15rpx 0px rgba(81, 81, 81, 0.05);
  background: #fff;
  position: relative;
}

.tab-box .tab {
  height: 100rpx;
  width: 50%;
  line-height: 100rpx;
  text-align: center;
  font-size: 28rpx;
  color: #777;
}

.tab-box .tab.active {
  color: #222;
  font-weight: bold;
}

.tab-box .line {
  width: 115rpx;
  height: 4rpx;
  background: #e6002d;
  position: absolute;
  bottom: 0;
  left: 130rpx;
  transition: left 0.2s linear;
}

.tab-box .line.active {
  left: 506rpx;
}

/* 分类 */

.swiper-wrapper {
  height: 100rpx;
  transition-property: -webkit-transform;
  display: flex;
  align-items: center;
  box-shadow: 0px 7rpx 15rpx 0px rgba(81, 81, 81, 0.05);
  margin-top: 10rpx;
}

.swiper-slide {
  height: 100%;
  flex-shrink: 0;
  float: left;
}

.slide-box {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
}

.swiper-slide .content {
  color: #777;
  display: flex;
  height: 100%;
  align-items: center;
}

.swiper-slide .content.active {
  color: #222;
  border-bottom: 4rpx solid #222;
}

/* 可使用优惠券 */
.list-box {
  padding-bottom: calc(70rpx + env(safe-area-inset-bottom));
}

.use-item {
  display: flex;
  flex-wrap: wrap;
  width: 690rpx;
  box-shadow: 6rpx 0 10rpx 5rpx rgba(81, 81, 81, 0.05);
  margin-top: 30rpx;
  margin-left: 30rpx;
  position: relative;
}

.use-item .left {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
  width: 250rpx;
  height: 200rpx;
  background-image: url("https://img.51jiabo.com/98b8c2b6-761c-4f35-8c66-a3eb4a182135.png");
  background-repeat: no-repeat;
  background-size: 100% 100%;
  color: #fff;
  padding-right: 20rpx;
}

.use-item .left .price {
  display: flex;
  align-items: baseline;
  font-size: 68rpx;
  font-weight: bold;
  margin-top: 4rpx;
}

.use-item .left .price .lab {
  font-size: 40rpx;
  font-weight: normal;
}

.use-item .left .cond {
  font-size: 22rpx;
  margin-top: -8rpx;
}

.use-item .right {
  width: calc(100% - 250rpx);
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 8rpx 0 8rpx 18rpx;
}

.use-item .right .title {
  font-size: 32rpx;
  max-width: 300rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 2;
  line-height: 40rpx;
}

.use-item .right .past {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 22rpx;
  color: #777777;
  width: 270rpx;
  line-height: 32rpx;
  margin-top: 6rpx;
}

.use-item .spread{
  width: 100%;
  font-size: 20rpx;
  color: #777;
  padding: 20rpx 30rpx;
  background-color: #fff;
}

.use-item .box {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding-right: 17rpx;
}
.use-item .box .past{
  margin-top: 0;
  width: 230rpx;
}

.use-item .status {
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx;
  height: 40rpx;
  color: #3283F6;
  border-radius: 20px;
  margin-bottom: -4rpx;
}

.use-item .type {
  display: none;
  width: 131rpx;
  height: 77rpx;
  position: absolute;
  top: 0;
  right: 0;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.use-item .type.brand {
  background-image: url("https://img.51jiabo.com/71d0497d-5dab-43de-af3c-09370c850af0.png");
}

/* 购买svip未使用抵扣券 */
.svip-discount .left {
  background-image: url("https://img.51jiabo.com/9a21eb75-6cd9-4d58-86e4-060ba9fd5649.png");
}

.svip-discount .type {
  display: block;
  background-image: url("https://img.51jiabo.com/3c1bf2b6-14aa-43a3-ad03-c498829b4bc3.png");
}

.svip-discount .status {
  color: #ED9005;
  border: 1px solid #ED8F05;
}

.svip-discount .cond {
  display: none;
}

/* svip平台券 */
.svip-platform .left {
  background-image: url("https://img.51jiabo.com/84c84b35-3ab1-43a1-9330-31fc25301a24.png");
}

.svip-platform .status {
  color: #FF4B1F;
}

.svip-platform .type {
  display: block;
  background-image: url("https://img.51jiabo.com/b6efeaaa-1269-48c2-91d8-10b6fc65838a.png");
}

/* svip商户券 */
.svip-brand .status {
  color: #3283F6;
  border: 1px solid #3283F6;
}

.svip-brand .type {
  display: block;
  background-image: url("https://img.51jiabo.com/eef2b466-078b-46ee-831b-13146748f6e0.png");
}

/* 熊猫币抽奖 */
.xmb-type .left {
  background-image: url("https://img.51jiabo.com/795a1ac1-3630-4066-9053-3cf624f7f366.png");
}

.xmb-type .status {
  color: #ff4b1f;
  border: 1px solid #ff4b1f;
}

.xmb-type .type {
  display: block;
  background-image: url("https://img.51jiabo.com/0c678c22-bf50-4261-a954-9f19e979f505.png");
}

