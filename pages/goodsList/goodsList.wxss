.reserve-goods, .quankuan-goods, .dingjin-goods {
  display: flex;
  flex-wrap: wrap;
  background: #fff;
  margin-top: 15rpx;
  padding-left: 30rpx;
}
.visib{
  visibility: hidden;
}
.item-box {
  width: 343rpx;
  border-bottom: 2rpx solid #f5f5f5;
  padding: 25rpx 0 39rpx 13rpx;
}

.item-box:nth-child(odd) {
  padding-left: 0;
  padding-right: 13rpx;
  border-right: 2rpx solid #f5f5f5;
}

.product-dic {
  width: 100%;
  line-height: 1.8em;
  padding-bottom: 20rpx;
  height: 95rpx;
  border-bottom: 1rpx solid #efefef;
  overflow: hidden;
  color: #222;
  font-size: 24rpx;
  font-weight: 400;
}

.buy-btn {
  width: 100%;
  background: #e6002d;
  height: 64rpx;
  color: #fff;
  font-weight: 200;
  font-size: 28rpx;
}
.buy-btn.gray {
  background: #d0d0d0;
}
.repertory {
  display: flex;
  justify-content: space-between;
  align-items: baseline;
  margin-top: -14rpx;
}

.payment {
  display: flex;
  align-items: baseline;
}

.onload {
  width: 100%;
  display: flex;
  justify-content: center;
  font-size: 20rpx;
  color: #999999;
  padding: 33rpx 0;
}

/* 非svip提示弹层 */
.not-svip-tips {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: center;
}

.not-svip-box {
  width: 620rpx;
  border-radius: 8rpx;
  background: #fff;
}

.not-svip-box .h6 {
  font-size: 36rpx;
  color: #000;
  text-align: center;
  padding: 72rpx 0 77rpx;
}

.btn-box {
  display: flex;
  justify-content: space-between;
  padding: 0 62rpx 53rpx;
}

.btn-box .cancel {
  width: 190rpx;
  height: 70rpx;
  border: 1px solid #aaa;
  border-radius: 3rpx;
  color: #000;
  font-size: 36rpx;
  text-align: center;
  line-height: 70rpx;
}

.btn-box .svip-pay {
  width: 256rpx;
  height: 70rpx;
  background: #e6002d;
  border-radius: 3rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 36rpx;
  color: #fff;
}

/* 预约提示弹层 */
.reserve-tips {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: center;
}

.reserve-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 620rpx;
  border-radius: 8rpx;
  background: #fff;
  padding-bottom: 61rpx;
}

.reserve-box .h6 {
  font-size: 36rpx;
  color: #000;
  text-align: center;
  line-height: 60rpx;
  padding: 72rpx 0 53rpx;
}

.reserve-box .cancel {
  width: 190rpx;
  height: 70rpx;
  background: #e6002d;
  border-radius: 3rpx;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
  line-height: 70rpx;
}