/* 公共样式 */

button::after {
  border: none;
}
.auth-login {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  padding: 0;
  z-index: 3;
  background: none;
}
.session {
  position: relative;
  background: #fff;
  height: 100%;
  padding-top: 5rpx;
  margin-top: 20rpx;
  overflow: hidden;
}

.session .session-title {
  padding: 36rpx 30rpx;
  align-items: center;
}

.separator {
  font-size: 40rpx;
  color: #777;
  margin: 0 20rpx;
}

.left {
  font-size: 40rpx;
  font-weight: 800;
  color: #222;
}

/* 公共样式 end */

/* components/banner/bannerDird.wxss */

.page-wrap {
  padding-bottom: 80rpx;
}

.row-wrap {
  position: relative;
  width: 100%;
  height: 315rpx;
  padding: 15rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  flex-wrap: wrap;
}

.row-item {
  width: 33%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  padding: 20rpx;
}

.row-item .text {
  font-size: 28rpx;
  color: #222;
  max-width: 200rpx;
  font-weight: 600;
  margin-top: 10rpx;
}

.row-item image {
  width: 100rpx;
  height: 100rpx;
}

/*  */

.banner {
  position: relative;
  height: 905rpx;
  width: 100%;
}

.banner-bg {
  position: absolute;
  z-index: -1;
  height: 645rpx;
  width: 100%;
}

.explain-pos {
  padding: 80rpx 30rpx 0;
  justify-content: space-between;
  align-items: center;
  color: #fff;
}

.pos {
  min-width: 140rpx;
  padding: 0 20rpx;
  height: 55rpx;
  border-radius: 50rpx;
  background: rgba(194, 0, 38, 1);
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 0 20rpx;
}

.explain {
  width: 200rpx;
  font-size: 30rpx;
  font-weight: 300;
}

.banner-content {
  position: absolute;
  z-index: 2;
  top: 160rpx;
  left: 20rpx;
  /* height: 685rpx; */
  width: 710rpx;
  background: #fff;
  border-radius: 8rpx;
  box-shadow: 0px 3px 10px 0px rgba(112, 112, 112, 0.1);
  overflow: hidden;
}

.user-info {
  position: relative;
  width: 100%;
  height: 150rpx;
  background: black;
  color: #fff;
  padding-left: 60rpx;
}

.user-bg {
  position: absolute;
  left: 0;
  top: 0;
}

.user-main {
  position: absolute;
  display: flex;
  align-items: center;
  width: 100%;
  z-index: 2;
  height: 150rpx;
}

.user-info .picture {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
}

.user-info .name {
  font-size: 38rpx;
  font-weight: 800;
  color: #fff;
  background: none;
  outline: none;
  border: none;
  list-style: none;
  line-height: 1;
  margin: 0;
  padding: 0;
  display: flex;
}

.user-center {
  font-size: 28rpx;
  color: #fdd6ad;
  margin-top: 4rpx;
}

.user-info .main {
  display: flex;
  flex-direction: column;
  margin-left: 34rpx;
  margin-top: 6rpx;
}

.user-info .main .user {
  align-items: center;
}

.user-info .main .tips {
  font-size: 28rpx;
  color: #fdd6ad;
}

.user-info .more {
  width: 40rpx;
  height: 40rpx;
  background: #fff;
  color: black;
  border-radius: 50%;
  font-size: 22rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-left: 14rpx;
}

.rights {
  padding-bottom: 40rpx;
}

.rights-tig {
  text-align: center;
  padding: 32rpx 0 0rpx;
}

.rights .title {
  height: 126rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  flex-wrap: nowrap;
}

.rights .title text {
  font-size: 40rpx;
  font-weight: 800;
  white-space: nowrap;
}
.rights .title image {
  width: 90rpx;
  height: 4rpx;
}

/*  */

.rights-item {
  margin-top: 32rpx;
  width: 100%;
  background: #fff;
  padding: 40rpx;
  height: 300rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-direction: column;
}

.rights-title {
  align-items: center;
  padding-bottom: 32rpx;
}

.rights-title image {
  width: 40rpx;
}

.rights-img {
  width: 670rpx;
  height: 160rpx;
}

/*  */

/* .session-title {
  border: 1px solid red;
} */

.session-title::after {
  display: block;
  content: '';
  position: absolute;
  left: 30rpx;
  right: 30rpx;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.term-list {
  font-weight: 200;
  font-size: 28rpx;
  color: #666;
  padding: 40rpx 40rpx 240rpx;
}

.term-list view {
  line-height: 1.8em;
}

/* 按钮样式*/

.btns {
  width: 690rpx;
  height: 98rpx;
  background: #e5002d;
  border-radius: 50rpx;
  margin: 60rpx auto;
  color: #fff;
  box-shadow: 0px 3px 10px 0px rgba(112, 112, 112, 0.5);
}

.btns-content {
  align-items: center;
}

.arrows {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #e5002d;
}

.btn {
  width: 100%;
  margin: 50rpx auto;
  font-size: 40rpx;
  font-weight: 200;
  align-items: center;
}

.more {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background: #fff;
  margin-left: 20rpx;
  padding: 2rpx 0 0 5rpx;
}

.reserve-btn {
  position: fixed;
  bottom: 140rpx;
  left: 30rpx;
  z-index: 9;
  margin: 0;
  font-size: 40rpx;
  font-weight: 800;
}

/*弹窗*/

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.8);
  z-index: 11;
  display: flex;
  justify-content: center;
  align-items: center;
}

.dialog {
  height: 720rpx;
  width: 690rpx;
  border-radius: 8rpx;
  overflow: hidden;
  background: #fff;
  padding: 34rpx;
}

.header {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  color: #999;
}

.content {
  margin-top: 30rpx;
}

.content .form-item {
  height: 105rpx;
  border-bottom: 1rpx solid #ececec;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.form-item input {
  flex: 1;
}

.place-holder {
  font-size: 28rpx;
}

.code-btn {
  font-size: 24rpx;
}

.def-bg {
  color: #e5002d;
}

.send-bg {
  color: #c5c5c5;
}

/* 模拟滚动条 */

/*滚动条*/

.bar {
  position: fixed;
  height: 96rpx;
  background: #fff;
  bottom: 0;
  width: 100%;
  display: flex;
  justify-content: space-around;
  font-size: 20rpx;
  border-top: 1rpx solid #c5c5c5;
}

.icon-home, .icon-ticket, .icon-my {
  flex: 1;
  flex-basis: auto;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 2rpx;
  /* border: 1px solid rebeccapurple; */
}

.bar image {
  width: 55rpx;
  height: 55rpx;
}

.select-color {
  color: #e5002d;
}

/* 弹层 */

.reserve-popup {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.7);
}

.reserve-box {
  width: 610rpx;
  border-radius: 8rpx;
  /* background: #fff; */
}

.reserve-box image {
  width: 100%;
  max-height: 900rpx;
  display: block;
}

.reserve-box .close {
  width: 84rpx;
  height: 84rpx;
  margin-left: 263rpx;
  margin-top: 40rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
