/* components/navigation/navigation.wxss */
.outsite-link-layer {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 10;
  transition: opacity 0.3s linear;
}

.outsite-link-layer.hide-layer {
  opacity: 0;
  display: none;
}

.outsite-link {
  height: 203rpx;
  position: fixed;
  right: 0;
  top: 380px;
  z-index: 11;
  display: flex;
  align-items: center;
  transition: right 0.3s linear;
}

.outsite-link.hide-link {
  right: -650rpx;
}

.pack-up {
  width: 82rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  flex-shrink: 0;
  font-size: 24rpx;
  color: #fff;
  background: #E6002D;
  border-radius: 60% 0 0 60%;
}

.pack-up .ico {
  width: 26rpx;
  height: 25rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}
.outsite{
  display: flex;
  align-items: center;
  height: 203rpx;
  background: #E6002D;
  border-radius: 8rpx 0 0 8rpx;
}
.outsite-box {
  display: flex;
  justify-content: center;
}

.outsite-box .link {
  width: 188rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 24rpx;
  border-right: 1px solid #ff819a;
}

.outsite-box .link:last-of-type {
  border: none;
}

.outsite-box .link .img {
  width: 54rpx;
  height: 59rpx;
  margin-bottom: 24rpx;
}

.unfold {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 152rpx;
  height: 102rpx;
  position: fixed;
  top: 789rpx;
  right: -40rpx;
  background: #e6002d;
  border-radius: 52rpx;
  font-size: 24rpx;
  color: #fff;
  z-index: 11;
  padding-left: 33rpx;
}
.unfold .arrow{
  width: 26rpx;
  height: 25rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  margin-left: 10rpx;
  margin-bottom: 5rpx;
}