/* components/couponItem/couponItem.wxss */

/* 优惠券 */

.vendor-coupon {
  padding: 35rpx 30rpx 0;
}

.fenxiang,.share {
  background-color: #fff;
  border-radius: 0rpx;
  padding: 0rpx;
  margin: 0rpx;
  display: inline-block;
  line-height: normal;
}

.fenxiang::after,.share::after {
  border: none;
}

.vendor-coupon .single {
  display: flex;
  flex-direction: column;
  border-radius: 10rpx;
  width: 690rpx;
  margin: 0 0 37rpx 0;
  text-align: left;
  background-color: #fff;
  overflow: hidden;
}

.vendor-coupon .single .top-bg{
  display: flex;
  align-items: center;
  width: 100%;
  height: 90rpx;
  background: url(https://img.51jiabo.com/3d14863f-1c01-4189-9d02-c8cbd2a092c6.jpg) no-repeat;
  background-size: cover;
  padding: 20rpx 0;
  overflow: hidden;
}

.vendor-coupon .single .top-bg .vendor-img{
  width: 100rpx;
  height: 50rpx;
  border-radius: 50rpx;
  overflow: hidden;
  margin: 0 30rpx;
}

.vendor-coupon .single .top-bg .ticket-title{
  font-size: 32rpx;
  font-weight: bold;
  color: #fff;
}

.vendor-coupon .single .above {
  height: 140rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.vendor-coupon .single .left{
  display: flex;
  align-items: center;
}

.vendor-coupon .single .below {
  margin: 0 23rpx;
  padding-right: 28rpx;
  border-top: 1px dashed #dcdcdc;
}

.vendor-coupon .single .btm-t {
  height: 66rpx;
  font-size: 20rpx;
  color: #777;
  line-height: 66rpx;
  display: flex;
  justify-content: space-between;
}

.vendor-coupon .single .below-msg{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 450rpx;
  padding-left: 20rpx;
}

.vendor-coupon .single .spread{
  width: 100%;
  font-size: 20rpx;
  color: #777;
  padding-left: 20rpx;
  padding-bottom: 30rpx;
}

.fenxiang {
  font-size: 24rpx;
  font-family: PingFang;
  font-weight: 500;
  color: #E6002D;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.fx-icon {
  width: 25rpx;
  height: 24rpx;
  margin-right: 11rpx;
}

.vendor-coupon .single .v-price {
  height: 100%;
  width: 210rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.vendor-coupon .single .v-price .p-top {
  font-size: 64rpx;
  font-weight: bold;
  color: #FF0036;
}

.vendor-coupon .single .ticket-info {
  width: 272rpx;
  font-size: 24rpx;
}

.vendor-coupon .single .ticket-info .yet-get{
  color: #222;
  margin-bottom: 10rpx;
}

.vendor-coupon .single .ticket-info .use-time {
  color: #777;
}

.vendor-coupon .single .get-ticket {
  width: 198rpx;
  height: 100%;
  display: flex;
  align-items: center;
}

.vendor-coupon .single .get-ticket .get-btn {
  width: 150rpx;
  height: 50rpx;
  background: #e6002d;
  color: #fff;
  font-size: 25rpx;
  border-radius: 25rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.vendor-coupon .single .get-ticket .get-btn.gray {
  background: #d0d0d0;
  color: #fff;
}

/* 索票成功弹层 */

.get-ticket-box {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.7);
}

.get-ticket-box .get-ticket {
  width: 620rpx;
  background: #fff;
  border-radius: 5rpx;
  position: relative;
  padding: 37rpx 0 56rpx;
}

.get-ticket-box .get-ticket .txt-box {
  text-align: center;
  border-bottom: 1px solid #dedede;
  padding-bottom: 33rpx;
}

.get-ticket-box .get-ticket .txt-box .tit1 {
  font-size: 42rpx;
  font-weight: bold;
}

.get-ticket-box .get-ticket .txt-box .tit2 {
  font-size: 28rpx;
  margin-top: 7rpx;
  padding: 0 60rpx;
}

.coupon-box {
  display: flex;
  width: 529rpx;
  height: 153rpx;
  box-shadow: 3px 0 15rpx 5rpx rgba(81, 81, 81, 0.05);
  margin-top: 43rpx;
  margin-left: 45rpx;
}

.coupon-box .left {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 192rpx;
  height: 153rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
  color: #fff;
}

.coupon-box .left .price {
  display: flex;
  align-items: baseline;
  font-size: 52rpx;
  font-weight: bold;
  margin-top: 4rpx;
}

.coupon-box .left .price .lab {
  font-size: 31rpx;
  font-weight: normal;
}

.coupon-box .left .cond {
  font-size: 17rpx;
  margin-top: -10rpx;
}

.coupon-box .right {
  display: flex;
  flex-direction: column;
  margin-left: 65rpx;
}

.coupon-box .right .s-img {
  width: 94rpx;
  height: 51rpx;
  border: 1px solid #e5e5e5;
  margin-top: 15rpx;
}

.coupon-box .right .name {
  font-size: 23rpx;
  margin-top: 8rpx;
}

.coupon-box .right .date {
  font-size: 17rpx;
  color: #666;
  margin-top: 2rpx;
}

.get-ticket .share {
  width: 530rpx;
  height: 83rpx;
  border-radius: 5rpx;
  background: #e6002d;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 83rpx;
  margin-top: 28rpx;
  margin-left: 45rpx;
}

.get-ticket .remaind {
  font-size: 24rpx;
  color: #777;
  margin-top: 44rpx;
  padding: 0 45rpx;
}

.get-ticket .remaind .remaind-txt {
  color: #e6002d;
  font-weight: bold;
}

.get-ticket .close {
  position: absolute;
  top: -50rpx;
  right: -50rpx;
  width: 99rpx;
  height: 99rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}