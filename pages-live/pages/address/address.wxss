.shopList{
  width: 100%;
  height:100rpx ;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #E6002D;
  font-size: 32rpx;
}
.address{
  width: 100%;
  padding: 0 30rpx;
  background: #fff;
  margin-top: 23rpx;
}
.address .addressList{
  padding: 35rpx 15rpx;
  line-height: 36rpx;
  font-size:26rpx ;
  color: #282828;
}
.address .addressList .shop{
  margin-bottom: 10rpx;
}
.endAddress{
  padding: 20rpx;
  text-align: center;
  font-size: 24rpx;
  color: #666;
  position: relative;
}
.endAddress:after,.endAddress:before{
  content: '';
  position: absolute; /*定位背景横线的位置*/
  top: 50%;
  width: 140rpx; /*单侧横线的长度*/
  height: 1px;
}
.endAddress:before{
  background: linear-gradient(90deg,rgba(255,255,255,1) 0%,rgba(173,173,173,1) 100%);
  left: 140rpx; /*调整背景横线的左右距离*/
}
.endAddress:after{
  right: 140rpx;
  background: linear-gradient(90deg,rgba(173,173,173,1) 0%,rgba(255,255,255,1) 100%);
}





