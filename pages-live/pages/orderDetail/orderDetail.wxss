  width: 640rpx;
  overflow: hidden;
}

.flag .codePopup .code text {
  text-align: center;
  display: block;
  font-size: 32rpx;
  line-height: 32rpx;
  padding-top: 22rpx;
}

.flag .codePopup .confirmCodeBtn {
  width: 190rpx;
  height: 70rpx;
  background: #E6002D;
  border-radius: 3rpx;
  color: #fff;
  text-align: center;
  line-height: 60rpx;
  margin: 0 auto;
  position: absolute;
  bottom: 67rpx;
  font-size: 36rpx;
  left: 50%;
