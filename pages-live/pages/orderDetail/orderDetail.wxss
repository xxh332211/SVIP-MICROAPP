}

.flag .refundPopup {
  width: 620rpx;
  background: #fff;
  border-radius: 8rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  padding-bottom: 50rpx;
}

.flag .refundPopup .title {
  width: 375rpx;
  margin: 80rpx auto 40rpx;
  font-size: 36rpx;
  line-height: 48rpx;
  text-align: center;
}

.flag .refundPopup .btnlist {
  width: 450rpx;
  height: 70rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;

}

.flag .refundPopup .btnlist view {
  width: 190rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 36rpx;
  border-radius: 4rpx;
}

.flag .refundPopup .btnlist view:nth-of-type(1) {
  border: 1rpx solid #AAAAAA;
  color: #000;
}

.flag .refundPopup .btnlist view:nth-of-type(2) {
  border: 1rpx solid #E6002D;
  color: #E6002D;
}

.flag .confirmPopup {
  width: 620rpx;
  background: #fff;
  border-radius: 8rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
  padding-bottom: 50rpx;
}

.flag .confirmPopup .title {
  width: 450rpx;
  margin: 94rpx auto 60rpx;
  font-size: 30rpx;
  line-height: 48rpx;
  text-align: center;
}

.flag .confirmPopup .know {
  width: 190rpx;
  height: 70rpx;
  background: rgba(230, 0, 45, 1);
  border-radius: 3rpx;
  font-size: 36rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 70rpx;
  margin: 0 auto;
}

