/* 待提货 */
.cargo {
  width: 100%;
  height: 190rpx;
  background: #E6002D;
  color: #fff;
  overflow: hidden;
}

.cargo .cargoTitle {
  width: 100%;
  height: 61rpx;
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.cargo .cargoTitle image {
  width: 68rpx;
  height: 61rpx;
}

.cargo .cargoTitle text {
  font-size: 36rpx;
  font-weight: bold;
  padding-left: 20rpx;
}

.cargo .cargo-hint {
  text-align: center;
  font-size: 30rpx;
  line-height: 30rpx;
  padding: 26rpx 0 33rpx;
}

/* 剩余时间 */
.remainDate {
  /* height: 150rpx; */
  width: 100%;
}

.remainDate .date {
  display: flex;
  justify-content: center;
  font-size: 38rpx;
  line-height: 38rpx;
  color: #222;
  font-weight: bold;
  padding-top: 33rpx;
}

.remainDate .date text:nth-of-type(2) {
  color: #FF0036;
  margin-left: 10rpx;
}

.remainDate .price {
  color: #FF0036;
  text-align: center;
  font-size: 36rpx;
  line-height: 36rpx;
  font-weight: bold;
  padding: 23rpx 0 32rpx;
}

/* 订单 */
.order {
  background: #fff;
  padding: 0 28rpx 0 30rpx;
  margin: 0rpx 0;
  width: 100%;
}

.order .orderNumber {
  height: 89rpx;
  font-size: 32rpx;
  line-height: 89rpx;
  font-weight: bold;
}

.order .main {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.order .main image {
  width: 200rpx;
  height: 153rpx;
}

.order .main .mainRight {
  position: relative;
  width: 450rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
}

.order .main .mainRight .title {
  font-size: 28rpx;
  font-weight: bold;
  line-height: 39rpx;
  color: #222;
}

.order .main .mainRight .detail {
  display: flex;
  align-items: center;
  line-height: 54rpx;
  font-size: 28rpx;
  font-family: PingFang;
  font-weight: bold;
  color: #222222;
  margin: 4rpx 0 -4rpx;
}

.order .main .mainRight .detail .right {
  font-weight: normal;
}

.order .main .mainRight .detail .price {
  font-size: 32rpx;
  color: #E6002D;
}

.order .main .mainRight .detail .border {
  width: 2rpx;
  height: 30rpx;
  background: #222;
  margin: 0 26rpx;
}

.order .total {
  height: 88rpx;
  border-top: 1px solid #EEEEEE;
  padding: 30rpx 0;
  margin-top: 30rpx;
  display: flex;
  justify-content: flex-end;
  align-items: flex-end;
}

.order .total text:nth-of-type(1) {
  color: #777777;
  font-size: 26rpx;
  line-height: 26rpx;
}

.order .total text:nth-of-type(2) {
  color: #E6002D;
  font-size: 26rpx;
  line-height: 26rpx;
  font-weight: bold;
}

/* 门店列表 */
.storeList {
  width: 100%;
}

.storeList .store_box {
  background: #fff;
  width: 100%;
  padding: 35rpx 30rpx 20rpx;
}

.storeList .store_box .header {
  display: flex;
  align-items: center;
  height: 36rpx;

}

.storeList .store_box .header .addressimg {
  width: 25rpx;
  height: 35rpx;
}

.storeList .store_box .header text {
  font-size: 34rpx;
  line-height: 34rpx;
  font-weight: bold;
  padding-left: 20rpx;
}

.storeList .store_box .storeTxt {
  width: 100%;
  /* border-bottom: 1rpx dashed #707070; */
  font-size: 24rpx;
  line-height: 36rpx;
  padding: 25rpx 40rpx;
  color: #282828;
}

.storeList .store_box .storeTxt .txt1 {
  margin-bottom: 10rpx;
}

.storeList .storeMore {
  width: 144rpx;
  height: 38rpx;
  background: linear-gradient(140deg, rgba(255, 0, 54, 1), rgba(255, 38, 0, 1));
  border-radius: 18rpx;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
  line-height: 38rpx;
  margin: 10rpx auto;
}

/* 提货码 */
.pickCode {
  background: #fff;
  padding: 30rpx;
  font-size: 30rpx;
  line-height: 100rpx;
  margin-top: 30rpx;
}

.pickCode .pickDate {
  color: #FF0036;
  line-height: 30rpx;
  padding-bottom: 30rpx;
}

.codeDetail {
  width: 100%;
  height: 80rpx;
  border-radius: 80rpx;
  background: #efefef;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #282828;
  margin-bottom: 30rpx;
}

.codeDetail .codeCount {
  margin: 0 42rpx 0 24rpx;
}

.codeDetail .state {
  color: #E6002D;
}

.codeBtn {
  display: flex;
  justify-content: flex-end;
  padding-right: 20rpx 0 30rpx;
  height: 100rpx;
  align-items: center;
}

.codeBtn view {
  height: 50rpx;
  border: 1rpx solid #959595;
  border-radius: 25rpx;
  color: #222;
  width: 120rpx;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.codeBtn .codetxt {
  margin-left: 22rpx;
}

/* 订单信息 */
.orderMessage {
  /* margin-top: 28rpx; */
  background: #fff;
  padding: 0 30rpx 40rpx;
}

.orderMessage .title {
  font-size: 32rpx;
  line-height: 90rpx;
  font-weight: bold;

}

.orderMessage .commonVal {
  display: flex;
  line-height: 54rpx;
}

.orderMessage .commonVal text:nth-of-type(1) {
  flex-shrink: 0;
  width: 135rpx;
  font-size: 30rpx;
  color: #000;
  font-size: 28rpx;
  font-family: PingFang;
  font-weight: 500;
  color: #333333;
}

.orderMessage .commonVal text:nth-of-type(2) {
  font-size: 30rpx;
  color: #282828;
  font-size: 28rpx;
  font-family: PingFang;
  font-weight: 400;
  color: #666666;
}

/* 确认支付 */
.confirmPay {
  width: 600rpx;
  height: 80rpx;
  background: rgba(230, 0, 45, 1);
  border-radius: 40rpx;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin: 50rpx auto 110rpx;
}

/* 弹窗 */
.flag {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .7);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9999;
}

.flag .codePopup {
  width: 640rpx;
  height: 790rpx;
  border-radius: 8rpx;
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background: url('https://img.51jiabo.com/f6b29ba5-b551-48e5-853b-0a09f0a11f7f.png') no-repeat;
  background-size: cover;
}

