  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.bag.show{
  right: 20rpx;
}
.bag{
  width: 135rpx;
  height: 116rpx;
  position: fixed;
  top: 30%;
  right:-160rpx;
  z-index: 8;
  transition: all .2s linear;
}