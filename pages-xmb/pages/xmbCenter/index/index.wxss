/* pages-xmb/pages/xmbCenter/xmbCenter.wxss */
.container {
  padding: 44rpx 15rpx 100rpx;
  box-sizing: border-box;
}

.banner-bg {
  width: 100%;
  height: 584rpx;
  position: absolute;
  left: 0;
  top: -144rpx;
  z-index: -1;
  color: #fff;
  background: url("https://img.51jiabo.com/0722a0fa-7ff7-47c1-87b3-d2ecb8f30ef7.png") no-repeat;
  background-size: 100% 100%;
}

.xmb-rules {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  color: #fff;
  font-size: 32rpx;
  padding-right: 15rpx;
  padding-bottom: 10rpx;
}

.num-title {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
}

.num-title>.num {
  font-size: 60rpx;
  font-weight: bold;
  margin: 0 16rpx 0 10rpx;
}

.sign-in {
  width: 100%;
  background-color: #fff;
  margin-top: 40rpx;
  padding: 38rpx 0 29rpx;
  border-radius: 10rpx;
  box-shadow: 0 10rpx 10rpx #ccc;
}

.sign-in-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #222;
  font-size: 34rpx;
  font-weight: bold;
  padding: 0 40rpx 0 30rpx;
}

.sign-in-progress {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  padding: 40rpx 59rpx;
}

.sign-in-progress>.pro-item {
  position: relative;
  z-index: 2;
  text-align: center;
  color: #B0B0B0;
}

.sign-in-progress>.pro-item-active {
  color: #FF052E;
}

.sign-in-progress>.pro-line {
  position: absolute;
  left: 70rpx;
  right: 70rpx;
  top: 55rpx;
  z-index: 1;
  border-bottom: 1rpx solid #F2F3F3;
}

.sign-in-tips {
  text-align: center;
  color: #999A9E;
  font-size: 24rpx;
}

.task {
  width: 100%;
  padding: 40rpx 30rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin-top: 30rpx;
}

.task-title {
  color: #222;
  font-size: 42rpx;
  font-weight: bold;
  margin-bottom: 40rpx;
}

.task-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #EFEFEF;
}

.task-main {
  display: flex;
  align-items: center;
}

.task-cont {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 100rpx;
  margin-left: 17rpx;
}

.task-cont-tit {
  color: #222;
  font-size: 30rpx;
  font-weight: bold;
}

.task-cont-pro{
  color: #B0B0B0;
  font-size: 24rpx;
  margin-left: 15rpx;
}

.task-cont-desc {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #B0B0B0;
}

.task-status {
  width: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.task-status-btn {
  width: 140rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  background-color: #FF0036;
  color: #fff;
  font-size: 24rpx;
  border-radius: 56rpx;
}

.xmb-use {
  width: 100%;
  padding: 40rpx 30rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin-top: 30rpx;
}

.lottery {
  margin-bottom: 50rpx;
}

.lottery-title {
  color: #222;
  font-size: 42rpx;
  font-weight: bold;
  margin-bottom: 30rpx;
}

.lottery-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  width: 640rpx;
  height: 200rpx;
  background: linear-gradient(to right, #FF304D, #FF8459);
  border-radius: 10rpx;
  padding: 20rpx;
  padding-right: 10rpx;
}

.lottery-cover {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 9;
}

.lottery-prize-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 470rpx;
  height: 120rpx;
  background: url(https://img.51jiabo.com/a20bccb2-c113-48b7-8f64-da5f90c08230.png) no-repeat;
  background-size: cover;
  padding: 10rpx 30rpx;
}

.lottery-left-swp {
  width: 95rpx;
  height: 85rpx;
}

.lottery-left-swp .swp-item {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 95rpx;
  height: 85rpx;
  border: 3rpx solid #ffddad;
  border-bottom-width: 6rpx;
  border-radius: 10rpx;
}

.lottery-left-swp .prize-img {
  width: 75rpx;
  height: 75rpx;
}

.lottery-notice {
  height: 30rpx;
  display: flex;
  align-items: center;
  transform: translateY(12rpx);
}

.lottery-notice-swp {
  width: 400rpx;
  height: 38rpx;
  margin-left: 10rpx;
  color: #fff;
}

.lottery-right {
  color: #fff;
  text-align: center;
}

.lottery-100 {
  display: table-cell;
  vertical-align: middle;
  width: 130rpx;
  height: 124rpx;
  background: url(https://img.51jiabo.com/a26e1136-394e-4442-9f31-618f58b535ae.png) no-repeat;
  background-size: cover;
  font-size: 30rpx;
  padding: 15rpx;
  line-height: 36rpx;
  transform: translateY(10rpx);
}

.lottery-check-prize {
  margin-top: 15rpx;
}

.activity-title {
  color: #222;
  font-size: 42rpx;
  font-weight: bold;
}

.activity-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: flex-end;
}

.activity-list:after {
  content: "";
  height: 0;
  width: 32%;
}

.activity-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 40rpx;
}

.activity-name {
  width: 200rpx;
  color: #222;
  font-size: 26rpx;
  margin: 15rpx 0;
  text-align: center;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.activity-cont {
  display: flex;
  align-items: center;
  color: #B0B0B0;
  font-size: 22rpx;
}

.activity-cont>.activity-num {
  color: #E6002D;
  font-size: 28rpx;
  font-weight: bold;
  margin-right: 10rpx;
}

.activity-cont>.activity-rules {
  display: flex;
  align-items: center;
}

.activity-btn {
  width: 140rpx;
  height: 47rpx;
  border-radius: 47rpx;
  line-height: 47rpx;
  background-color: #FF0036;
  color: #F2F2F2;
  font-size: 28rpx;
  margin-top: 12rpx;
}

.cover {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9;
  background-color: rgba(0, 0, 0, .8);
}

.freeze-box {
  width: 600rpx;
  height: 300rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  padding: 80rpx 73rpx;
  font-size: 32rpx;
  color: #222;
  line-height: 64rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.scan-box {
  background-color: #fff;
  border-radius: 10rpx;
  position: absolute;
  top: 50%;
  left: 35rpx;
  right: 35rpx;
  transform: translateY(-50%);
  padding: 50rpx 0 40rpx;
}

.scan-title {
  font-size: 42rpx;
  margin-bottom: 30rpx;
  text-align: center;
}

.canvas {
  width: 200rpx;
  height: 200rpx;
  margin: auto;
}

.scan-desc {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  margin: 20rpx 0;
}

.scan-line {
  width: 100%;
  height: 30rpx;
  background-color: #EFEFEF;
}

.scan-list {
  height: 450rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.scan-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 110rpx;
  padding: 0 15rpx;
  font-size: 28rpx;
  color: #222;
  border-bottom: 1rpx solid #EFEFEF;
}

.scan-item-status {
  color: #222;
  text-align: center;
  font-size: 26rpx;
}

